{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build-zengo",
      "command": "dotnet",
      "type": "process",
      "args": ["build", "${workspaceFolder}/Zengo/Zengo.csproj"],
      "problemMatcher": "$msCompile",
      "dependsOn": []
    },
    {
      "label": "build-zengo-csv",
      "command": "dotnet",
      "type": "process",
      "args": ["build", "${workspaceFolder}/Zengo.Csv/Zengo.Csv.csproj"],
      "problemMatcher": "$msCompile",
      "dependsOn": ["build-zengo"]
    },
    {
      "label": "build-zengo-excel",
      "command": "dotnet",
      "type": "process",
      "args": ["build", "${workspaceFolder}/Zengo.Excel/Zengo.Excel.csproj"],
      "problemMatcher": "$msCompile",
      "dependsOn": ["build-zengo"]
    },
    {
      "label": "build-zengo-json",
      "command": "dotnet",
      "type": "process",
      "args": ["build", "${workspaceFolder}/Zengo.Json/Zengo.Json.csproj"],
      "problemMatcher": "$msCompile",
      "dependsOn": ["build-zengo"]
    },
    {
      "label": "build-tests",
      "command": "dotnet",
      "type": "process",
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "args": ["build", "${workspaceFolder}/Tests/Tests.csproj"],
      "problemMatcher": "$msCompile",
      "dependsOn": ["build-zengo-csv", "build-zengo-excel", "build-zengo-json"]
    }
  ]
}
